<?xml version="1.0" encoding="UTF-8"?>
<simplepdl:Process
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:simplepdl="http://simplepdl"
    xsi:schemaLocation="http://simplepdl ../../fr.enseeiht.gls.simplepdl/SimplePDL.ecore"
    name="simple">
  <processElements
      xsi:type="simplepdl:WorkDefinition"
      linksToSuccessors="//@processElements.2"
      name="cook"/>
  <processElements
      xsi:type="simplepdl:WorkDefinition"
      linksToPredecessors="//@processElements.2"
      name="eat"/>
  <processElements
      xsi:type="simplepdl:WorkSequence"
      linkType="finishToStart"
      predecessor="//@processElements.0"
      successor="//@processElements.1"/>
  <processElements
      xsi:type="simplepdl:Ressource"
      name="plat"
      quantite="1"/>
  <processElements
      xsi:type="simplepdl:Ressource_Usage"
      used_ressource="//@processElements.3"
      user_WD="//@processElements.0"
      quantite="1"/>
  <processElements
      xsi:type="simplepdl:Ressource_Usage"
      used_ressource="//@processElements.3"
      user_WD="//@processElements.1"
      quantite="1"/>
</simplepdl:Process>
